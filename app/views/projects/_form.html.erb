
<%= form_for @project do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :priority %>
  <%= f.text_field :priority %>
  <br />

  <%= f.label :to, 'Assigned to' %>
  <%= f.text_field :to %>
  <br />

  <%= f.label :frontend %>
  <%= f.text_field :frontend %>
  <br />

  <%= f.label :backend %>
  <%= f.text_field :backend %>
  <br />

  <%= f.label :customer %>
  <%= f.text_field :customer %>
  <br />

  <br>

  <%= f.submit %>
<% end  %>
