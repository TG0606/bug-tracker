
<%= form_for @project do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :priority %>
  <%= f.text_field :priority %>
  <br />
  <%= f.label :frontend %>
  <%= f.text_field :frontend %>
  <br />

  <%= f.label :backend %>
  <%= f.text_field :backend %>
  <br />

  <%= f.label :customer %>
  <%= f.text_field :customer %>
  <br />

  <br>

  <% User.all.each do |u| %>
    <%= f.check_box :user_ids, { multiple: true}, u.id, false  %>
    <%= u.name %>
    <br />
  <% end %>
  <br />
  <%= f.submit %>
<% end  %>
