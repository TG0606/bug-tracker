

<%= form_for @project do |f| %>

  <tr>
    <td class="labels"><%= f.label :name %></td>
    <td class="labels_box"><%= f.text_field :name %></td>
  </tr>

  <tr>
    <td class="labels">
      <%= f.label :priority %></td>
    <td class="labels_box">
    <%= f.select :priority,['Low','Medium','High'] %>
    </td>
  </tr>



  <tr>
    <td class="labels"><%= f.label :to,'Assigned to' %></td>
    <td class="labels_box"><%= f.text_field :to %></td>
  </tr>

  <tr>
    <td class="labels"><%= f.label :frontend %></td>
    <td class="labels_box"><%= f.text_field :frontend %></td>
  </tr>

  <tr>
    <td class="labels"><%= f.label :backend %></td>
    <td class="labels_box"><%= f.text_field :backend %></td>
  </tr>

  <tr>
    <td class="labels"><%= f.label :customer %></td>
    <td class="labels_box"><%= f.text_field :customer %></td>
  </tr>

  <br>

  <% User.all.each do |u| %>
    <%= f.check_box :user_ids, { multiple: true}, u.id, false  %>
    <%= u.name %>
    <br />
  <% end %>
  <br />
  <%= f.submit %>
<% end  %>
